                                return;
                            }
                            if (listFiles.length > IMAGE_MAX_COUNT) {
                                $('#file_qa').text(upload_img_count_error).show();
                                return;
                            }
                            var form_data = new FormData();
                            form_data.append('imgFile', file);
                            $.ajax({
                                url: '/ajax/ajaxupload',
                                dataType: 'text',
                                cache: false,
                                contentType: false,
                                processData: false,
                                data: form_data,
                                type: 'post',
                                success: function (res) {
                                    $('#select_file').val('');
                                    if (!!res) {
                                        json = JSON.parse(res);
                                        if (json['error'] != '') {
                                            $('#file_qa').text(json['error']).show();
                                        } else {
                                            listFiles.push(json['filename']);
                                            $('#arealf').append('<span  style="position: relative;" id="' + json['filename'] + '" ><img class="img-thumbnail" src="' + baseUrl + 'upload/' + json['filename'] + '"   ><i class="icon-upload-remove-image" onclick="rmf(this);"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></i></span>');
                                            $('#uploadImage').val(JSON.stringify(listFiles));
                                            duplicate.push(nameOfFile);
                                        }
                                    } else {
                                        $('#file_qa').text(upload_img_fail_error).show();
                                    }
                                    $("#select_file").val('');
                                }
                            });
                        }
                    });
                    $("#addnewqa").submit(function (event) {
                        $('#btn_sendqa').prop('disabled', true);
                    });

                    $('#btn_sendqa').click(function () {
                        check = 0;
                        var title = $("input[name='form_sendqa[title]']").val();
                        var option = $("select[name='form_sendqa[option]']").val();
                        var content = $("#content_qas").val();
                        var check_login = $('#login_check').val();
                        if (check_login == '') {
                            var alertMsg = "Vui lòng đăng nhập/đăng ký để đặt câu hỏi tại Ecomedic.vn";
                            showAlertNotLogin(alertMsg, 2000);
                            saveQA();
                            return false;
                        }
                        if (title == '') {
                            check = 1;
                            document.getElementById("validate_title").innerHTML = "Xin vui lòng nhập tiêu đề câu hỏi";
                        } else {
                            if (title.length > 256) {
                                check = 1;
                                document.getElementById("validate_title").innerHTML = "Tiêu đề của bạn quá dài";
                            }
                        }
                        if (content == '') {
                            check = 1;
                            document.getElementById("content_qa").innerHTML = "Xin vui lòng nhập nội dung câu hỏi";
                        }
                        if (option == '') {
                            check = 1;
                            document.getElementById("validate_option_sp").innerHTML = "Xin vui lòng chọn chuyên mục câu hỏi";
                        }
                        if (check == 1) {
                            return false;
                        }

                        localStorage.setItem("option", "newest");
                        //if submit ok --> remove lqa
                        localStorage.removeItem("lqa");
                    });
                    $('#title').keyup(function () {
                        document.getElementById("validate_title").innerHTML = "";
                    });
                    $('#option_sp').change(function () {
                        var title = $(this).val()
                        if (title != '') {
                            document.getElementById("validate_option_sp").innerHTML = "";
                        }
                    });
                    $('#content_qas').keyup(function () {
                        document.getElementById("content_qa").innerHTML = "";
                    });
                });


                function rmf(obj) {
                    var name = $(obj).parent().attr('id');
                    $(obj).parent().remove();
                    var newListF = [];
                    for (var i = 0; i < listFiles.length; i++) {
                        if (listFiles[i] != name) {
                            newListF.push(listFiles[i]);
                        } else {
                            duplicate[i] = '';
                        }
                    }
                    listFiles = newListF;
                    $('#uploadImage').val(JSON.stringify(listFiles));
                    $('#file_qa').hide();
                    console.log($('#uploadImage').val());
                }
            </script>
        </div>
    </div>
</div>

